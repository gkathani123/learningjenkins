pipeline {
    agent {
        docker { image 'guruathani/myrepo' 
		withDockerContainer(args: '--name test -it', image: 'guruathani/myrepo') {
    sh 'apic --version'
}
		}
    }
    stages {
        stage('Test') {
            steps {
                sh 'apic --version'
            }
        }
    }
}
